    <xs:element name="getRelanceByIdRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getRelanceByIdResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="relance" type="tns:relanceWS"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

